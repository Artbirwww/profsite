{
  "compilerOptions": {
    "jsx": "react-jsx",
    "allowJs": false,
    "moduleResolution": "bundler",
    "module": "ESNext",
    "target": "ES2020",
    "strict": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "outDir": "./build",
    "rootDir": "./src"
  },
  "include": ["src"]
}